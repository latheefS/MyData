                        ,pt_i_ProcOrFuncName      => cv_ProcOrFuncName
                        ,pt_i_ProgressIndicator   => gvv_ProgressIndicator
                        ,pt_i_ModuleMessage       => 'Extracting data into "' || cv_StagingTable   || '" Name '||p_bg_name                   
                        ,pt_i_OracleError         => gvt_ReturnMessage       );
        --
        INSERT  
        INTO    XXMX_THIRD_ADDR_STG
                (migration_set_id
                ,migration_set_name                   
                ,migration_status
                ,bg_name
                ,bg_id           
				,registry_id	
				,party_id  
				,organizationname	
				,country	
				,addressline1	
				,addressline2	
				,addressline3
				,addressline4
				,city	
				,county	
				,province	
				,state	
				,postalcode	)
        SELECT   pt_i_MigrationSetID                               
                ,pt_i_MigrationSetName                               
                ,'EXTRACTED'  
                ,p_bg_name
                ,p_bg_id
				,hp.party_number
				,hp.party_id
				,hp.party_name
				,HP.COUNTRY
				,HP.ADDRESS1
				,HP.ADDRESS2
				,HP.ADDRESS3
				,HP.ADDRESS4
				,HP.CITY
				,HP.COUNTY			
				,HP.PROVINCE
				,HP.STATE
				,HP.POSTAL_CODE
		FROM HZ_ORGANIZATION_PROFILES@MXDM_NVIS_EXTRACT haou, 
			HZ_PARTIES@MXDM_NVIS_EXTRACT HP 
		WHERE   HP.party_type = 'ORGANIZATION'
		AND third_party_flag = 'Y'
        AND TRUNC(SYSDATE) BETWEEN haou.EFFECTIVE_START_DATE AND NVL(haou.EFFECTIVE_END_DATE,'31-DEC-4712')
		AND HP.party_id = haou.party_id 
		UNION 
		SELECT pt_i_MigrationSetID                               
                ,pt_i_MigrationSetName                               
                ,'EXTRACTED'  
                ,p_bg_name
                ,p_bg_id
                ,hp.party_number
				,hp.party_id
				,hp.party_name
				,HP.COUNTRY
				,HP.ADDRESS1
				,HP.ADDRESS2
				,HP.ADDRESS3
				,HP.ADDRESS4
				,HP.CITY
				,HP.COUNTY			
				,HP.PROVINCE
				,HP.STATE
				,HP.POSTAL_CODE
		FROM HZ_Parties@MXDM_NVIS_EXTRACT HP
			,HZ_person_profiles@MXDM_NVIS_EXTRACT  HPP   
		where trunc(sysdate) BETWEEN HPP.effective_start_date AND NVL(HPP.effective_end_date,'31-DEC-4712')
		AND HPP.Party_id = HP.Party_id 
		AND HP.party_id IN(
							SELECT HR.Subject_id
							--, HR.subject_type 
							FROM HZ_PARTIES@MXDM_NVIS_EXTRACT HP,
								HZ_Relationships@MXDM_NVIS_EXTRACT HR       
							WHERE HR.object_id = HP.party_id
							AND HP.party_type IN('PERSON')
							AND HR.object_type = 'PERSON'
							AND HR.subject_type = 'PERSON');

        COMMIT;
        --
        gvv_ProgressIndicator := '0030';
        xxmx_utilities_pkg.log_module_message(  
                        pt_i_ApplicationSuite    => gct_ApplicationSuite
